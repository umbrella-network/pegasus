{
  "name": "pegasus",
  "version": "6.4.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/umbrella-network/pegasus.git"
  },
  "scripts": {
    "lint": "eslint ./src/**/*.ts ./test/**/*.ts --max-warnings=0",
    "lint:fix": "eslint ./src/**/*.ts ./test/**/*.ts --fix",
    "prettier": "prettier --config .prettierrc ./src/**/*.ts ./test/**/*.ts --check",
    "prettier:format": "prettier --config .prettierrc ./src/**/*.ts ./test/**/*.ts --write",
    "format": "npm run lint:fix && npm run prettier:format",
    "clean": "rm -rf dist",
    "compile": "tsc -p .",
    "copy:assets": "cpx 'src/**/*.{graphql,html,png,json}' dist",
    "start:dev": "tsnd --respawn src/server.ts",
    "start:dev:worker": "tsnd --respawn src/worker.ts",
    "start:dev:agent": "tsnd --respawn src/agent.ts",
    "start:dev:scheduler": "tsnd --respawn src/scheduler.ts",
    "task:dev": "tsnd --rs src/tasks.ts",
    "build": "npm run clean && npm run compile && npm run copy:assets",
    "start": "node dist/server.js",
    "start:worker": "node dist/worker.js",
    "start:agent": "node dist/agent.js",
    "start:scheduler": "node dist/scheduler.js",
    "task": "node dist/tasks.js",
    "test": "cross-env NODE_ENV=testing mocha",
    "list:top:cryptopairs": "ts-node src/scripts/listTopCryptoPairs.ts"
  },
  "devDependencies": {
    "@types/chai": "~4.2.14",
    "@types/chai-as-promised": "~7.1.3",
    "@types/compression": "~1.7.0",
    "@types/cookie-parser": "~1.4.2",
    "@types/cors": "~2.8.12",
    "@types/express": "~4.17.8",
    "@types/google-protobuf": "^3.7.4",
    "@types/js-yaml": "~4.0.1",
    "@types/lodash": "~4.14.178",
    "@types/mocha": "~9.1.0",
    "@types/moxios": "~0.4.9",
    "@types/newrelic": "~7.0.0",
    "@types/node": "~16.7.10",
    "@types/object-hash": "~1.3.4",
    "@types/sinon": "~10.0.6",
    "@types/swagger-ui-express": "~4.1.3",
    "@types/uuid": "~8.3.0",
    "@types/websocket": "~1.0.1",
    "@types/yamljs": "~0.2.31",
    "@types/yargs": "~15.0.9",
    "@typescript-eslint/eslint-plugin": "~4.6.1",
    "@typescript-eslint/parser": "~4.6.1",
    "chai": "~4.2.0",
    "cpx": "~1.5.0",
    "cross-env": "~7.0.3",
    "eslint": "~7.13.0",
    "mocha": "~9.1.0",
    "moxios": "~0.4.0",
    "node-statsd-client": "~1.0.4",
    "prettier": "~2.3.0",
    "sinon": "~12.0.1",
    "statsd-client": "~0.4.5",
    "ts-node": "~10.4.0",
    "ts-node-dev": "~1.1.8",
    "typescript": "~4.0.5"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.3.7",
    "@kaiko-data/sdk-node": "0.0.33",
    "@newrelic/publish-sourcemap": "~5.0.1",
    "@typegoose/typegoose": "~8.3.0",
    "@types/node-schedule": "~1.3.1",
    "@umb-network/toolbox": "umbrella-network/canvas#test-release/5.7.0",
    "@umb-network/validator": "~0.4.0",
    "async-mutex": "^0.3.2",
    "axios": "~0.21.1",
    "bignumber.js": "~9.0.1",
    "bullmq": "~1.34.0",
    "chai-as-promised": "~7.1.1",
    "compression": "~1.7.4",
    "cors": "~2.8.5",
    "dayjs": "~1.10.7",
    "dotenv": "~8.2.0",
    "ethereumjs-util": "~7.0.10",
    "ethers": "~5.4.5",
    "express": "~4.17.1",
    "fast-sort": "~2.2.0",
    "google-protobuf": "^3.17.3",
    "helmet": "~4.2.0",
    "inversify": "~5.0.1",
    "ioredis": "~4.19.2",
    "js-combinatorics": "1.5.4",
    "js-yaml": "~3.14.1",
    "jsonpath-plus": "~4.0.0",
    "jsonschema": "~1.4.0",
    "lodash": "~4.17.21",
    "mongoose": "~5.13.11",
    "newrelic": "~7.2.0",
    "node-cache": "^5.1.2",
    "node-schedule": "~2.0.0",
    "object-hash": "~2.1.1",
    "reflect-metadata": "~0.1.13",
    "swagger-ui-express": "~4.1.6",
    "uuid": "~8.3.1",
    "websocket": "~1.0.33",
    "winston": "~3.3.3",
    "yamljs": "~0.3.0",
    "yargs": "~16.1.0"
  }
}
